{
  "title": "Frequently Asked Questions",
  "plugins": [
    "theme-faq",
    "-sharing",
    "image-captions",
    {
      "name": "disqus",
      "version": "git+https://github.com/kilbot/plugin-disqus.git"
    },
    {
      "name": "versions",
      "version": "git+https://github.com/kilbot/plugin-versions.git"
    }
  ],
  "pluginsConfig": {
    "versions": {
      "type": "branches",
      "container": ".faq-header .container"
    },
    "disqus": {
      "shortName": "woocommercepos",
      "container": ".faq-page-container .panel-body"
    }
  }
}