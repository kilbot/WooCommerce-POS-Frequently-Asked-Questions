{
  "title": "Frequently Asked Questions",
  "plugins": [
    "-sharing",
    "-search",
    "ga",
    "image-captions",
    {
      "name": "theme-faq",
      "version": "git+https://github.com/kilbot/theme-faq.git"
    },
    {
      "name": "disqus",
      "version": "git+https://github.com/kilbot/plugin-disqus.git"
    },
    {
      "name": "versions",
      "version": "git+https://github.com/kilbot/plugin-versions.git"
    },
    {
      "name": "hints",
      "version": "git+https://github.com/kilbot/plugin-hints.git"
    },
    {
      "name": "sitemap",
      "version": "git+https://github.com/kilbot/plugin-sitemap.git"
    }
  ],
  "pluginsConfig": {
    "versions": {
      "type": "branches",
      "container": ".faq-header .container"
    },
    "disqus": {
      "shortName": "woocommercepos",
      "container": ".faq-page-container .panel-body"
    },
    "sitemap": {
      "hostname": "http://faq.wcpos.com"
    },
    "ga": {
      "token": "UA-65534-23"
    }
  }
}